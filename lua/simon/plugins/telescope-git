return {
  "nvim-telescope/telescope.nvim",
  event = "VeryLazy",
  dependencies = {
    {
      "isak102/telescope-git-file-history.nvim",
      dependencies = { "tpope/vim-fugitive" }
    }
  },

  config = function()
    vim.keymap.set("n", "<leader>fgo", require("telescope").extensions.git_file_history.git_file_history(),
      { description = "Git file history" })
  end

}
